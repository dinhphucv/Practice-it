public static double percentEven(int[] numbers) {
    if (numbers.length == 0) {
        return 0.0;
    }
    int evenNumbers = 0;
    for (int i = 0; i <= numbers.length - 1; i++) {
        if (numbers[i] % 2 == 0) {
            evenNumbers++;
        }
    }
    return evenNumbers * 100.0 / numbers.length;
}
